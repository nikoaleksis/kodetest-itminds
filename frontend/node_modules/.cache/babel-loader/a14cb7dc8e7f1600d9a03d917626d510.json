,OAAO,CAACU,WAAR,EAAb,CARoB,CASpB;;AACA,UAAIX,OAAO,CAACY,KAAR,CAAc,GAAd,EAAmB,CAAnB,MAA0BC,SAA9B,EAAyC;AACvC,yBAAUT,IAAV,cAAkBI,KAAlB,cAA2BE,IAA3B;AACF;;AACA,UAAMI,KAAI,GAAIT,MAAM,CAACJ,OAAO,CAACc,QAAR,EAAD,CAAN,CAA2BR,QAA3B,CAAoC,CAApC,EAAuC,CAAvC,CAAd;AACA,UAAMS,OAAM,GAAIX,MAAM,CAACJ,OAAO,CAACgB,UAAR,EAAD,CAAN,CAA6BV,QAA7B,CAAsC,CAAtC,EAAyC,CAAzC,CAAhB;AACA,uBAAUH,IAAV,cAAkBI,KAAlB,cAA2BE,IAA3B,cAAmCI,KAAnC,cAA4CE,OAA5C;AACD,KAjBM;AAkBPE,eAlBO,yBAkBO;AACZC,aAAO,CAACC,GAAR,CAAY,MAAZ;AACD;AApBM,GAFI;AAwBbC,MAxBa,kBAwBN;AACL,WAAO;AACLzB,WAAK,EAALA;AADK,KAAP;AAGD;AA5BY,CAAf","names":["notes","name","methods","parseDateStr","dateStr","dateObj","Date","slice","date","String","getDate","padStart","month","getMonth","year","getFullYear","split","undefined","hours","getHours","minutes","getMinutes","setIsClosed","console","log","data"],"sourceRoot":"","sources":["C:\\cygwin64\\home\\nol.it-minds.no\\kodetest\\kpmg\\kodetest-7april\\src\\note\\ReviewNote.vue"],"sourcesContent":["<template>\n  <div class=\"content\">\n    <div class=\"page-title\">\n      <h1>Review Notes</h1>\n      <button class=\"new-note\">New</button>\n    </div>\n    <div class=\"filter\">\n      <input type=\"text\" placeholder=\"Quick search\">\n      <label for=\"toggle-type\">Type</label>\n      <label for=\"toggle-priority\">Priority</label>\n      <label for=\"reporters\">Reporter</label>\n      <input id=\"reporters\" type=\"text\">\n      <label for=\"assignees\">Assignees</label>\n      <input id=\"assignees\" type=\"text\"/>\n      <label for=\"section\">Section</label>\n      <input id=\"section\" type=\"text\"/>\n      <label for=\"date\">Date</label>\n      <input id=\"date\" type=\"text\"/>\n    </div>\n    <div class=\"data\">\n      <table>\n        <tbody>\n          <tr v-for=\"(note, index) in notes\" :key=\"index\">\n            <td>\n              <input\n              type=\"checkbox\"\n              v-model=\"note.isClosed\"\n              @:change=\"setIsClosed\">\n            </td>\n            <td>{{note.title}}</td>\n            <td>{{note.type}}</td>\n            <td>{{note.status}}</td>\n            <td>{{note.priority.text}}</td>\n            <td>{{parseDateStr(note.dueDate)}}</td>\n            <td>{{note.reporterId.$oid}}</td>\n            <td v-if=\"note.assignees.length !== 0\">\n              <span v-for=\"(assignee, index) in note.assignees\" :key=\"index\">\n                {{assignee.$oid}}\n              </span>\n            </td>\n            <td v-else></td>\n            <td>{{note.sectionRef}}</td>\n            <td>{{parseDateStr(note.createdAt.$date)}}</td>\n            <!--<td>{{parseDate(note.updatedAt.$date)}}</td>-->\n          </tr>\n        </tbody>\n      </table>\n    </div>\n  </div>\n</template>\n\n<script>\nimport notes from '../data/notes';\n\nexport default {\n  name: 'ReviewNotes',\n  methods: {\n    parseDateStr(dateStr) {\n      if (dateStr === '') {\n        return dateStr;\n      }\n      // Create a date object remove the zero hour offset if exists\n      const dateObj = new Date(dateStr.slice(-1) === 'Z' ? dateStr.slice(0, -1) : dateStr);\n      const date = String(dateObj.getDate()).padStart(2, 0);\n      const month = String(dateObj.getMonth()).padStart(2, 0);\n      const year = dateObj.getFullYear();\n      // If date string has no time, return only dd.mm.yyyy\n      if (dateStr.split('T')[1] === undefined) {\n        return `${date}.${month}.${year}`;\n      }\n      const hours = String(dateObj.getHours()).padStart(2, 0);\n      const minutes = String(dateObj.getMinutes()).padStart(2, 0);\n      return `${date}.${month}.${year} ${hours}:${minutes}`;\n    },\n    setIsClosed() {\n      console.log('Test');\n    },\n  },\n  data() {\n    return {\n      notes,\n    };\n  },\n};\n</script>\n\n<style scoped>\ntable {\n  width: 100%;\n}\n.content {\n  width: 100%;\n}\n.page-title {\n  display: flex;\n  border-bottom: 2px solid #eee;\n  margin-bottom: 20px;\n}\n.filter {\n  margin-bottom: 50px;\n}\n.new-note {\n  background-color: blue;\n  color: #fff;\n  height: 50px;\n  width: 125px;\n}\n</style>\n"]}]}